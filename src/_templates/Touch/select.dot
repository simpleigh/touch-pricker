/**
 * Renders a touch as HTML for use within a <select> element
 */

<option value="0">
	{{= Pricker.stringFromRow(context.object.getInitialRow()) }}
</option>

{{~ context.object.getCourses() :course }}
	<option value="{{= course.getOwnership()[1] }}">
		{{= course.print('text') }}
	</option>
{{~}}
