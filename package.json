    "karma-jasmine": "5.1.0",
    "karma-jsdom-launcher": "15.0.0",
    "karma-webpack": "5.0.0",
    "ts-loader": "9.4.4",
    "typedoc": "0.24.8",
    "typescript": "5.1.6",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "webpack-merge": "4.2.2"
