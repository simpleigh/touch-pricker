    "dist",
    "CHANGELOG.md",
    "COPYING",
    "COPYING.GPL",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:dev && yarn build:prod && yarn build:test",
    "build:dev": "webpack --config config/webpack.config.dev.js",
    "build:prod": "webpack --config config/webpack.config.prod.js",
    "build:test": "webpack --config config/webpack.config.test.js",
    "doc": "node_modules/.bin/typedoc src/index.ts",
    "lint": "eslint . --ext .js,.ts",
    "start": "webpack-dev-server --config config/webpack.config.dev.js",
    "start:tests": "webpack-dev-server --config config/webpack.config.test.js",
    "test": "karma start --browsers jsdom",
    "test:browsers": "karma start --single-run"
  },
  "devDependencies": {
    "@jsdevtools/coverage-istanbul-loader": "3.0.5",
    "@types/jasmine": "4.3.5",
