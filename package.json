    "@typescript-eslint/parser": "6.2.1",
    "clean-webpack-plugin": "4.0.0",
    "codecov": "3.8.3",
    "copy-webpack-plugin": "11.0.0",
    "dot": "1.1.3",
    "dotjs-loader": "0.1.0",
    "eslint": "8.46.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jsdoc": "46.4.5",
