{
  "name": "touch-pricker",
  "version": "1.1.0",
  "description": "Free Touch Pricker",
  "main": "dist/touch-pricker.js",
  "jsnext:main": "dist/touch-pricker.js",
  "browser": "dist/touch-pricker.js",
