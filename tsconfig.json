{
    "compilerOptions": {
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "newLine": "LF",
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noPropertyAccessFromIndexSignature": true,
        "noUnusedLocals": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "target": "ES6"
    }
}
